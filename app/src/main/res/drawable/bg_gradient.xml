<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#fffafafa" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:angle="0"
                android:startColor="#40B00020"
                android:centerColor="#40FFFFFF"
                android:endColor="#4064DD17"
                android:type="linear" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:angle="270"
                android:startColor="#fffafafa"
                android:centerColor="#f0fafafa"
                android:endColor="#00fafafa"
                android:type="linear" />
        </shape>
    </item>
</layer-list>